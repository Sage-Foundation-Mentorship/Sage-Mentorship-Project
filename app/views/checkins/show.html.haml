%p#notice= notice


%p
  %b Mentor:
  = @checkin.mentor.name
%p
  %b School:
  = @checkin.school.name

%p
  %b Checkin time:
  = @checkin.checkin_time.strftime("%m/%d/%Y %H:%M:%S")
%p
  %b Latitude:
  = @checkin.lat
%p
  %b Longitude:
  = @checkin.lon

%p
  %b Checkout time:
  = @checkout.checkout_time.strftime("%m/%d/%Y %H:%M:%S") unless @checkout.nil?
%p
  %b Latitude:
  = @checkout.lat unless @checkout.nil?
%p
  %b Longitude:
  = @checkout.lon unless @checkout.nil?



= link_to 'Edit', edit_checkin_path(@checkin)
\|
= link_to 'Back', checkins_path
